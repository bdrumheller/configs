# use semicolon for right - skhd --observe
shift + alt -  0x29 : yabai -m window --swap east
shift + alt - j : yabai -m window --swap west
shift + alt - k : yabai -m window --swap south
shift + alt - l : yabai -m window --swap north

# focus window
alt - j : yabai -m window --focus west
alt - k : yabai -m window  --focus south
alt - l : yabai -m window  --focus north
alt -0x29 : yabai -m window  --focus east

# kill window
shift + alt - q : yabai -m window --close

# option + return to open the terminal
alt - return : open -na /Applications/iTerm.app


# create an open mode
:: open @

alt - o ; open
open < alt  - o ; default
open < return ; default

open < v : open /Applications/VLC.app
open < c : open -na "/Applications/Google Chrome.app"
open < alt - c : open -na "/Applications/Google Chrome.app" --args --incognito
open < d : open -na "/Applications/Backup and Sync.app"
open < p : open -na "/Applications/Pycharm CE.app"
open < t : open -na "/System/Applications/TextEdit.app"
open < alt - m : open "/System/Applications/Maps.app"
open < n : open -na "/System/Applications/News.app"
open < m : open "/Applications/Amazon Music.app"
open < h : open "/System/Applications/Home.app"
open < f : open -na "/Applications/Firefox.app"
